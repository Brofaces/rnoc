<div class="container">
  <div class="navbar">
    <div class="navbar-inner navbar-fixed-top">
      <div class="pull-left">
        <a href="/"><img src="<%= RNOC[:logo] %>" id="rnoc-show-logo"></a>
      </div>
      <div class="pull-right">
        <h4><i>RNOC</i> - <%= @org.title %></h4>
      </div>
    </div>
  </div>
</div>


<% if notice %>
  <div class="container">
    <div class="alert alert-info"><%= notice %></div>
  </div>
<% end %>


<div class="container">
  <br>
  <% if !@pages.empty? %>
  <% @pages.each do |page| %>
  <p><i class="icon-fire icon-white"></i> &nbsp;
    <font class="home">
      <%= link_to page.title, page_path(page) %>
      <% if !page.enabled %>
      (Disabled)
      <% end %>
    </font>
  </p>
  <% end %>
  <% else %>
  <p><font class="home">No pages to list. Please add a page.</font></p>
  <% end %>
</div>


<div class="container">
  <div class="navbar">
    <div class="navbar-inner navbar-fixed-bottom">
      <!-- Main action bar buttons -->
      <ul class="nav">
        <li>
          butts
            <i class="icon-plus icon-black"></i>
        </li>
      </ul>

      <!-- Help/More info button -->
      <ul class="nav pull-right">
        <li>
          <a id="rnoc-help"
	     class="btn btn-small"
	     rel="tooltip"
	     title="help"><i class="icon-question-sign icon-black"></i> </a>
        </li>
      </ul>
    </div>
  </div>
</div>
